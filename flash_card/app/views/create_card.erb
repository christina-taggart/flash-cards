<div class="container">
  <a href='/'>Home</a>
  <h1>Create a new card!</h1>
  <form action='/deck/<%= @deck_id %>/card/create' method='post'>
    <fieldset>
      <li>
        <label>Question</label>
        <input ... name="question"/></li>
      <li>
        <label>Answer</label>
        <input ... name="answer"/></li>
      <li>
        <label>Category</label>
        <input ... name="category"/></li>
    </fieldset>
    <input type="submit" value="Create!"/>
  </form>
</div>